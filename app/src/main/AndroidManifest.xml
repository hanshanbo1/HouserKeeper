<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.edu.nbl.houserkeeper">

    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.BLUETOOTH" />
    <uses-permission android:name="android.permission.CALL_PHONE" />
    <uses-permission android:name="android.permission.KILL_BACKGROUND_PROCESSES" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.CAMERA" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".mian.MainActivity" />
        <activity android:name=".mian.LeadActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".mian.LogoActivity" />

        <service android:name=".service.MusicService" />

        <activity android:name=".mian.AboutActivity" />
        <activity android:name=".mian.SettingActivity" />
        <activity android:name=".mian.SpeedActivity" />
        <activity android:name=".mian.SoftmgrActivity" />
        <activity android:name=".mian.PhonemgrActivity" />
        <activity android:name=".mian.TelmgrActivity" />
        <activity android:name=".mian.FilemgrActivity" />
        <activity android:name=".mian.SdcleanActivity" />
        <activity android:name=".mian.TelmgrshowActivity" />
        <activity android:name=".mian.SoftmgrShowActivity" />
        <activity android:name=".mian.FilemgrShowActivity" />
    </application>

</manifest>